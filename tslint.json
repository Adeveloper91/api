{
  "rules": {
    "ban-comma-operator": false,
    "max-classes-per-file": false,
    "no-console": false,
    "no-implicit-dependencies": [
      true,
      "dev"
    ],
    "no-submodule-imports": false,
    "no-unused-expression": false
  },
  "extends": [
    "tslint:latest",
    "tslint-config-prettier"
  ]
}